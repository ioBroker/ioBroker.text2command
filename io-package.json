  "instanceObjects": [
    {
      "_id": "text",
      "type": "state",
      "common": {
        "name": "Text to process",
        "type": "string",
        "role": "text",
        "read": true,
        "write": true,
        "desc": "Write your text here with ack=false, and you will get answer with ack=true"
      },
      "native": {}
    },
    {
      "_id": "response",
      "type": "state",
      "common": {
        "name": "Answer on requested question",
        "type": "string",
        "role": "text",
        "read": true,
        "write": false,
        "desc": "Answer for written into text2command.0.text text"
      },
      "native": {}
    },
    {
      "_id": "error",
      "type": "state",
      "common": {
        "name": "Error text",
        "type": "string",
        "role": "value.error",
        "read": true,
        "write": false
      },
      "native": {}
    }
  ]
